# Exemplo de arquivo .env para produção
# Copie este arquivo para .env e preencha os valores

# Postgres
DB_USER=postgres
DB_PASSWORD=postgres
DB_NAME=conecta_unifesspa
DB_PORT=5432
DB_HOST=localhost

# Prisma connection
DATABASE_URL="postgresql://${DB_USER}:${DB_PASSWORD}@${DB_HOST}:${DB_PORT}/${DB_NAME}?schema=public"

# Application
PORT=3333
NODE_ENV=production

# JWT
JWT_SECRET=sua_chave_secreta_muito_segura_aqui

# pgAdmin
PGADMIN_DEFAULT_EMAIL=admin@conectauniesspa.com
PGADMIN_DEFAULT_PASSWORD=senha_segura_pgadmin
PGADMIN_PORT=5050

